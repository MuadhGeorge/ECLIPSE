{
  "framework": null,
  "buildCommand": "bash scripts/vercel-build.sh",
  "outputDirectory": "dist",
  "installCommand": "echo 'No npm install needed'",
  "headers": [
    {
      "source": "/(.*).wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    },
    {
      "source": "/(.*).data",
      "headers": [
        {
          "key": "Content-Type", 
          "value": "application/octet-stream"
        }
      ]
    }
  ]
}
